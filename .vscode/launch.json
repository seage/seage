{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch AssignmentProvider",
      "request": "launch",
      "mainClass": "org.seage.problem.qap.AssignmentProvider",
      "projectName": "seage-problem-qap"
    },
    {
      "type": "java",
      "name": "Launch FspProblemProvider",
      "request": "launch",
      "mainClass": "org.seage.problem.fsp.FspProblemProvider",
      "projectName": "seage-problem-fsp"
    },
    {
      "type": "java",
      "name": "Launch FspAntColonyTest",
      "request": "launch",
      "mainClass": "org.seage.problem.fsp.antcolony.FspAntColonyTest",
      "projectName": "seage-problem-fsp"
    },
    {
      "type": "java",
      "name": "Launch JspAntColonyTest",
      "request": "launch",
      "mainClass": "org.seage.problem.jsp.antcolony.JspAntColonyTest",
      "projectName": "seage-problem-jsp"
    },
    {
      "type": "java",
      "name": "Launch JspAlgorithmTest",
      "request": "launch",
      "mainClass": "org.seage.problem.jsp.antcolony.JspAlgorithmTest",
      "projectName": "seage-problem-jsp"
    },
    {
      "type": "java",
      "name": "Launch JspGeneticAlgorithmTest",
      "request": "launch",
      "mainClass": "org.seage.problem.jsp.genetics.JspGeneticAlgorithmTest",
      "projectName": "seage-problem-jsp"
    },
    {
      "type": "java",
      "name": "Debug Launcher (list)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": "list"
    },
    {
      "type": "java",
      "name": "Debug Launcher (metadata)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "metadata",
        "-p",
        "QAP",
        "-n",
        "1"
      ]
    },
    {
      "type": "java",
      "name": "Debug Launcher (experimenter, h2)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-single-random",
        "-p",
        "TSP",
        "-a",
        "GeneticAlgorithm",
        "-i",
        "berlin52",
        "-t",
        "1",
        "-n",
        "1"
      ]
    },
    {
      "type": "java",
      "name": "Debug Launcher (experimenter, postgres)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-single-random",
        "-p",
        "TSP",
        "-a",
        "GeneticAlgorithm",
        "-i",
        "berlin52",
        "-t",
        "1",
        "-n",
        "1"
      ],
      "env": {
        "DB_URL": "jdbc:postgresql://localhost/seage"
      }
    },
    {
      "type": "java",
      "name": "Debug Launcher (HH1 experimenter, postgres)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-approach",
        "-a",
        "HyperHeuristic1",
        "-i",
        "TSP:hyflex",
        "-t",
        "10",
        "-n",
        "1"
      ],
      "env": {
        "DB_URL": "jdbc:postgresql://localhost/seage"
      }
    },
    {
      "type": "java",
      "name": "Launch MetadataGenerator",
      "request": "launch",
      "mainClass": "org.seage.sandbox.MetadataGenerator",
      "projectName": "seage-launcher"
    },
    {
      "type": "java",
      "name": "Launch AssignmentProvider",
      "request": "launch",
      "mainClass": "org.seage.problem.qap.AssignmentProvider",
      "projectName": "seage-problem-qap",
      "cwd": "${workspaceFolder}",
      "args": ["seage-problems/discrete/qap/src/main/resources/org/seage/problem/qap/instances/tai12a.dat"]
    },

  ]
}
