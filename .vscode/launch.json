  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Debug Launcher (list)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": "list"
    },
    {
      "type": "java",
      "name": "Debug Launcher (experimenter, h2)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-single-random",
        "-p",
        "TSP",
        "-a",
        "GeneticAlgorithm",
        "-i",
        "berlin52",
        "-t",
        "1",
        "-n",
        "1"
      ]
    },
    {
      "type": "java",
      "name": "Debug Launcher (experimenter, postgres)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-single-random",
        "-p",
        "TSP",
        "-a",
        "GeneticAlgorithm",
        "-i",
        "berlin52",
        "-t",
        "1",
        "-n",
        "1"
      ],
      "env": {
        "DB_URL": "jdbc:postgresql://localhost/seage"
      }
    },
    {
      "type": "java",
      "name": "Debug Launcher (approach experimenter, h2)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-approach",
        "-a",
        "GeneticAlgorithm",
        "-i",
        "TSP:berlin52",
        "-t",
        "1",
        "-n",
        "1"
      ]
    },
    {
      "type": "java",
      "name": "Launch MetadataGenerator",
      "request": "launch",
      "mainClass": "org.seage.sandbox.MetadataGenerator",
      "projectName": "seage-launcher"
    },
    {
      "type": "java",
      "name": "Launch HelloSparkApp",
      "request": "launch",
      "mainClass": "org.seage.hh.spark.HelloSparkApp",
      "projectName": "seage-hyperheuristics"
    },
    {
      "type": "java",
      "name": "Launch HsqldbTest",
      "request": "launch",
      "mainClass": "org.seage.hh.knowledgebase.HsqldbTest",
      "projectName": "seage-hyperheuristics"
    },
    {
      "type": "java",
      "name": "Launch App",
      "request": "launch",
      "mainClass": "org.seage.hh.spark.App",
      "projectName": "seage-hyperheuristics"
    },
    {
      "type": "java",
      "name": "Debug (Launch) - Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-HsqldbTest<seage-knowledgebase>",
      "request": "launch",
      "mainClass": "org.seage.knowledgebase.HsqldbTest",
      "projectName": "seage-knowledgebase"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-ClasspathTest<seage-launcher>",
      "request": "launch",
      "mainClass": "org.seage.sandbox.ClasspathTest",
      "projectName": "seage-launcher"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-H2Test<seage-launcher>",
      "request": "launch",
      "mainClass": "org.seage.sandbox.H2Test",
      "projectName": "seage-launcher"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-ProcessorInfo<seage-launcher>",
