{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch FspAntColonyTest",
      "request": "launch",
      "mainClass": "org.seage.problem.fsp.antcolony.FspAntColonyTest",
      "projectName": "seage-problem-fsp"
    },
    {
      "type": "java",
      "name": "Launch JspAntColonyTest",
      "request": "launch",
      "mainClass": "org.seage.problem.jsp.antcolony.JspAntColonyTest",
      "projectName": "seage-problem-jsp"
    },
    {
      "type": "java",
      "name": "Debug Launcher (info)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": "info"
    },
    {
      "type": "java",
      "name": "Debug Launcher (details)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "details",
        "-p",
        "SAT"
      ]
    },
    {
      "type": "java",
      "name": "Debug Launcher (metadata)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "metadata",
        "-p",
        "QAP",
        "-n",
        "1"
      ]
    },
    {
      "type": "java",
      "name": "Debug Launcher (experimenter, h2)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-single-feedback",
        "-a",
        "TabuSearch",
        "-i",
        "TSP:berlin52",
        "-t",
        "100",
        "-n",
        "10"
      ]
    },
    {
      "type": "java",
      "name": "Debug Launcher (approach, h2)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-approach",
        "-a",
        "AntColony",
        "-i",
        // "JSP:ft10",
        "TSP:berlin52",
        "-i",
        "JSP:ft10",
        "-t",
        "60",
        "-n",
        "1"
      ],
      "env": {
        "LOG_LEVEL": "debug",
        // "DB_URL": "jdbc:postgresql://localhost:25432/seage"
      }
    },
    {
      "type": "java",
      "name": "Debug Launcher (experimenter, postgres)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-single-random",
        "-a",
        "GeneticAlgorithm",
        "-i",
        "TSP:berlin52",
        "-t",
        "1",
        "-n",
        "1"
      ],
      "env": {
        "DB_URL": "jdbc:postgresql://localhost:25432/seage"
      }
    },
    {
      "type": "java",
      "name": "Debug Launcher (HH1 experimenter, postgres)",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-approach",
        "-a",
        "HyperHeuristic1",
        "-i",
        "TSP:hyflex",
        "-t",
        "10",
        "-n",
        "1"
      ],
      "env": {
        "DB_URL": "jdbc:postgresql://localhost:25432/seage"
      }
    }
  ]
}
