{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "SatAntColonyTest",
      "request": "launch",
      "mainClass": "org.seage.problem.sat.antcolony.SatAntColonyTest",
      "projectName": "seage-problem-sat"
    },
    {
      "type": "java",
      "name": "SatAntColonyTestSimple",
      "request": "launch",
      "mainClass": "org.seage.problem.sat.antcolony.SatAntColonyTestSimple",
      "projectName": "seage-problem-sat"
    },
    {
      "type": "java",
      "name": "Run info",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": "info"
    },
    {
      "type": "java",
      "name": "Run details",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "details",
        "-p",
        "SAT"
      ]
    },
    {
      "type": "java",
      "name": "Run report",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "report"
      ]
    },
    {
      "type": "java",
      "name": "Run metadata",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "metadata",
        "-p",
        "QAP",
        "-n",
        "1"
      ]
    },
    {
      "type": "java",
      "name": "Run default experimenter, psql",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "env": {
        "DB_URL": "jdbc:postgresql://snoopy.lan:15432/seage"
      },
      "args": [
        "experiment-single-default",
        "-a",
        "AntColony",
        "-i",
        "SAT:uf100-01",
        "-t",
        "20",
        "-n",
        "10",
        "-s",
        "0"
      ]
    },
    {
      "type": "java",
      "name": "Run feedback experimenter, psql",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "env": {
        "DB_URL": "jdbc:postgresql://snoopy.lan:15432/seage"
      },
      "args": [
        "experiment-single-feedback",
        "-a",
        // "GeneticAlgorithm",
        // "TabuSearch",
        "AntColony",
        // "SimulatedAnnealing",
        "-i",
        "SAT:uf20-01",
        "-t",
        "20",
        "-n",
        "10",
        "-s",
        "0"
      ]
    },
    {
      "type": "java",
      "name": "Run experiment-approach, h2",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-approach",
        "-a",
        "AntColony",
        "-i",
        "TSP:berlin52",
        "-i",
        "JSP:ft10",
        "-t",
        "60",
        "-n",
        "1"
      ],
      "env": {
        "LOG_LEVEL": "debug"
      }
    },
    {
      "type": "java",
      "name": "Run experiment-single-random, postgres",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-single-random",
        "-a",
        "GeneticAlgorithm",
        "-i",
        "TSP:berlin52",
        "-t",
        "1",
        "-n",
        "1"
      ],
      "env": {
        "DB_URL": "jdbc:postgresql://localhost:25432/seage"
      }
    },
    {
      "type": "java",
      "name": "Run HH1 experiment-approach, postgres",
      "request": "launch",
      "mainClass": "org.seage.launcher.Launcher",
      "projectName": "seage-launcher",
      "cwd": "${workspaceFolder}",
      "args": [
        "experiment-approach",
        "-a",
        "HyperHeuristic1",
        "-i",
        "TSP:hyflex",
        "-t",
        "10",
        "-n",
        "1"
      ],
      "env": {
        "DB_URL": "jdbc:postgresql://localhost:25432/seage"
      }
    }
  ]
}
