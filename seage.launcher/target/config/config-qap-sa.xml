<?xml version="1.0" encoding="UTF-8"?>

<Config  xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
   xmlns='http://xml.seage.org/schema'
   xsi:schemaLocation='http://xml.seage.org/schema config.xsd'>

    <Problem id="QAP">
	<Instance name="tai12a" type="file" path="data/qap/tai12a.dat"/>
        <Visualizer outPath="output/tai12a.png" width="800" height="600"/>
    </Problem>

    <Algorithm id="SimulatedAnnealing">
        <Parameters
            annealCoeficient="0.99"
            initSolutionType="random"
            maxInnerIterations="2000"
            maxTemperature="1000"
            minTemperature="10"
            numInnerSuccesses="20"
            numSolutions="1"/>
    </Algorithm>

</Config>
