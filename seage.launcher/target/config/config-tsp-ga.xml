<?xml version="1.0" encoding="UTF-8"?>

<Config  xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
   xmlns='http://xml.seage.org/schema'
   xsi:schemaLocation='http://xml.seage.org/schema config.xsd'>

    <Problem id="TSP">
	<Instance name="eil51" type="file" path="data/tsp/eil51.tsp"/>
        <Visualizer outPath="output/eil51.png" width="800" height="600"/>
    </Problem>

    <Algorithm id="GeneticAlgorithm">
        <Parameters 
            numSolutions="100"
            iterationCount="500"
            crossLengthPct="40"
            eliteSubjectPct="10"
            mutateSubjectPct="5"
            mutateLengthPct="10"
            randomSubjectPct="2"/>
    </Algorithm>

</Config>
